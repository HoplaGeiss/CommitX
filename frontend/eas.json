{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "buildConfiguration": "Release"
      },
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "SENTRY_ORG": "gabriel-muller",
        "SENTRY_PROJECT": "commitx-mobile",
        "EXPO_PUBLIC_DEV_MODE": "true"
      }
    }
  },
  "submit": {
    "production": {}
  }
}