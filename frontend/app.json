{
  "expo": {
    "name": "CommitX",
    "slug": "commitx",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "privacy": "public",
    "supportUrl": "https://hoplageiss.github.io/CommitX/support.html",
    "privacyUrl": "https://hoplageiss.github.io/CommitX/privacy-policy.html",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.commitx.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.commitx.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a056ba00-715b-4d5d-a41c-ba0b876fedca"
      }
    },
    "plugins": []
  }
}
